let adminmid = (req, res, next) => {
    req.query.name = "bapu don"
    next()
}

const adminpost = (req, res, next) => {
    req.body.name = "jay zalavadiy"
    next()
}

const adminparams = (req, res, next) => {
    req.params.id = 'hellow'
    next()
}



module.exports = { adminmid, adminpost, adminparams }   